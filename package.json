{
  "name": "wife-app",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run --passWithNoTests",
    "typecheck": "tsc --noEmit",
    "convex:check": "convex codegen",
    "lint": "eslint .",
    "format": "prettier",
    "check": "prettier --write . && eslint --fix",
    "feedback": "pnpm run typecheck && pnpm run convex:check && pnpm run test && pnpm run lint",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "deploy": "[ -n \"$CONVEX_DEPLOY_KEY\" ] && convex deploy; vite build && wrangler deploy"
  },
